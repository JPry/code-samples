{
	"name": "woocommerce/grow-github-actions-hook-documentation",
	"description": "Grow Hook Documentation",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Automattic\\WooCommerce\\Grow\\GitHubActions\\HookDocumentation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Automattic\\WooCommerce\\Grow\\GitHubActions\\HookDocumentation\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/pest",
		"test:coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage --coverage-html coverage"
	},
	"require": {
		"php": ">=7.4",
		"symfony/finder": "^5.4"
	},
	"require-dev": {
		"pestphp/pest": "^1.23"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		},
		"archive-format": "zip",
		"notify-on-install": false,
		"sort-packages": true
	},
	"archive": {
		"name": "grow-github-actions-hook-documentation",
		"exclude": [
			"build",
			"coverage",
			"tests",
			"composer.json",
			"composer.lock",
			"phpunit.xml.dist"
		]
	}
}
